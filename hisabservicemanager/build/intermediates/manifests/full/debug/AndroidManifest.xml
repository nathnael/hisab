<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emmajack.hisab.hisabservicemanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="24" />

    <application
        android:name="com.emmajack.hisab.hisabservicemanager.App"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@android:drawable/presence_online"
        android:label="@string/app_name_am"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.emmajack.hisab.hisabservicemanager.activity.HomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.emmajack.hisab.hisabservicemanager.activity.HomeActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.emmajack.hisab.hisabservicemanager.activity.CommingSoon"
            android:parentActivityName="com.emmajack.hisab.hisabservicemanager.activity.HomeActivity" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>